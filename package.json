    "st-babel": "yarn add @babel/plugin-proposal-decorators babel-plugin-module-resolver babel-plugin-transform-remove-console -D",
    "st-layout": "yarn add @react-navigation/stack @react-navigation/native @react-navigation/bottom-tabs react-native-elements",
    "st-plus": "yarn add react-native-animatable react-native-modal",
    "podM1": "cd ios/ &&  arch -x86_64 npx pod-install && cd .."
